<script>

var prog = document.getElementById("program").value,
cent = document.getElementById("center").value,
data = [],
questKey = {'A': 'A. I am treated with respect', 'B':'B. I would recommend BFC',
            'C':'C. Staff do what they say they will do', 'D': 'D. People here care about me',
            'E':'E. BFC has made a difference in my life','F':'F. Staff listen to me', 
            'G':'G. Staff understand and respect my culture', 'H':'H. I understand what I am told by staff',
            'I':'I. My privacy and confidentiality are respected','J':'J. Staff shows a high level of knowledge/experience',
            'K':'K. BFC provides services my community needs','L':'L. I receive the education/support I need'};

var margin = {top: 20, right: 75, bottom: 30, left: 40},
  width = 750 - margin.left - margin.right,
  height = 550 - margin.top - margin.bottom;

var x = d3.scale.ordinal()
  .rangeRoundBands([0,width],.1);

var y = d3.scale.linear()
  .rangeRound([height,0]);

var color = d3.scale.ordinal()
  .range(["#7A1213", "#7A1213", "#DD3B3B", "#aeb5a5", "#aeb5a5"]);

var xAxis = d3.svg.axis()
  .scale(x)
  .orient("bottom");

var yAxis = d3.svg.axis()
  .scale(y)
  .orient("left")
  .tickFormat(d3.format(".0%"));

var svg = d3.select("#chart").append("svg")
    .attr("width", width + margin.left + margin.right + 50)
    .attr("height", height + margin.top + margin.bottom + 200)
  .append("g")
    .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

var question = svg.selectAll(".question");
var labelOne,
labelThree,
labelFive;


data = filterdata();

//console.log(data);


 svg.append("g")
      .attr("class", "x axis")
      .attr("transform", "translate(0," + height + ")")
      .call(xAxis);

 d3.selectAll(".x .tick text")
   .attr('dx',10)
    //.attr('dy',70)
    .attr('font-size',12)
    .attr('transform','rotate(45)')
    .style('text-anchor','start');

  svg.append("g")
      .attr("class", "y axis")
      .call(yAxis);

  
 question = question.data(data)
    .enter().append("g")
      .attr("class", "question")
      .attr("id",function(d){return d.Question;})
      .attr("transform", function(d) { return "translate(" + x(d.Question) + ",0)" ; });

  question.selectAll("rect")
      .data(function(d) { return d.rating; })
      .enter().append("rect")
      .attr("width", x.rangeBand())
      .attr("y", function(d) { return y(d.y1); })
      .attr("height", function(d) { return y(d.y0) - y(d.y1); })
      .attr("class",function(d){ return d.name;})
      .style("fill", function(d) { return color(d.name); });

 labelOne = question.append('svg:text')
 .attr('y', function(d){return y(d.rating[0].y0)})
 .attr('dy',function(d){return 2+(y(d.rating[1].y1) - y(d.rating[0].y0))/2})
 .attr('dx',x.rangeBand()/2-8)
 .attr('class','one')
  .text(function(d){return Math.round((d.groupings['1-2']/d.groupings.Total)*100) + '%';})
  .style('fill','white');

labelThree = question.append('svg:text')
  .attr('y', function(d){ return y(d.rating[2].y0); })
  .attr('dy',function(d){return 2+(y(d.rating[2].y1)-y(d.rating[2].y0))/2})
  .attr('dx',x.rangeBand()/2-8)
  .attr('class','five')
    .text(function(d){ return Math.round((d.groupings['3']/d.groupings.Total)*100)+'%';})
    .style('fill','white');

labelFive = question.append('svg:text')
  .attr('y', function(d){ return y(d.rating[3].y0); })
  .attr('dy',function(d){return 2+(y(d.rating[4].y1)-y(d.rating[3].y0))/2})
  .attr('dx',x.rangeBand()/2-10)
  .attr('class','three')
    .text(function(d){ return Math.round((d.groupings['4-5']/d.groupings.Total)*100)+'%';})
    .style('fill','white');
  


d3.selectAll("select").on("change",update);


//defined functions
function filterdata(){

//if(dat === undefined){ console.log(dat);}else{
  var dat;


  dat = satisfied13.filter(function(d){
   

    if(d.Center ==  cent && d.Program == prog){
 
      return d;
    
    }

  });

  x.domain(dat.map(function(d) { return questKey[d.Question]; }));

  color.domain(d3.keys(dat[0]).filter(function(key){ 

    if( key.length == 1 ) return key;

  }));  


  dat.forEach(function(d){
    var y0 = 0;
    
    d.rating = color.domain().map(function(name){ //for each rating in a given row add an array of objects to that row with bottom height and top height for each rating
      
      return {name: name, y0: y0, y1: y0 += +d[name]};
  
  });

  d.rating.forEach(function(d){ d.y0 /= y0; d.y1 /= y0;});

  d.groupings = {'1-2':parseInt(d[1])+parseInt(d[2]), '3': parseInt(d[3]), '4-5':parseInt(d[4]) + parseInt(d[5]), 
                'Total':parseInt(d[1])+parseInt(d[2])+parseInt(d[3])+parseInt(d[4])+parseInt(d[5])};
 
});

return dat;
//}

};

 
 function update(){

  prog = document.getElementById("program").value;
  cent = document.getElementById("center").value;


  if(prog == 'Medical' || prog == 'RPP' || prog == 'Clothing') {

    cent = 'All';
    $('.charttip').css('display','inline');
    //document.getElementById("center").value = 'All';

  } else {$('.charttip').hide();}
//console.log(data);
  data = filterdata();
 //console.log(data);
  question.data(data).selectAll("rect")
    .data(function(d){return d.rating;}).transition()
    .duration(750)
    .attr("y", function(d) { return y(d.y1); })
    .attr("height", function(d) { return y(d.y0) - y(d.y1); })
    .attr('stroke','none');

labelOne.data(data)
  .transition()
  .duration(750)
   .attr('y', function(d){return y(d.rating[0].y0)})
 .attr('dy',function(d){return 2+(y(d.rating[1].y1) - y(d.rating[0].y0))/2})
 .text(function(d){ 
    var per = Math.round((d.groupings['1-2']/d.groupings.Total)*100);
    if(per > 0){
    return  per + '%';
  } else {return '';}
  });

 labelThree.data(data)
  .transition()
  .duration(750)
   .attr('y', function(d){return y(d.rating[2].y0)})
